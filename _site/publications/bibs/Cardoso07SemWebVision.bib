@ARTICLE{Cardoso07SemWebVision,  author = {Cardoso, Jorge},  title = {The Semantic Web Vision: Where Are We?},  journal = {IEEE Intelligent Systems},  year = {2007},  volume = {22},  pages = {84-88},  number = {5},  abstract = {The article discusses the state of ongoing efforts to implement Semantic	Web (Web 3.0), as indicated by a survey conducted among several hundred	Semantic Web academics and practitioners. Protege continues to be	the most popular ontology editor, with Swoop a distant second. OWL	and RDF Schema are the two most popular ontology languages. Education,	computer software, and government were the top three areas for which	survey participants developed ontologies. On-To-Knowledge was the	most popular methodology used for developing ontologies among those	who use a methodology, although 60% of respondents don't use one.},  address = {jcardoso@uma.pt Professor, University of Madeira},  issn = {15411672},  owner = {Jorge Cardoso},  refid = {174},  timestamp = {2010.08.26},  url = {http://ieeexplore.ieee.org/xpl/tocresult.jsp?isnumber=5552580},  webpdf = {/Papers/JA-2007-013-IEEE-IS-sw-survey.pdf}}